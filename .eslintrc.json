{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": "eslint:recommended",
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "linebreak-style": "off",
    "quotes": ["error", "double"],
    "camelcase": "off",
    "no-console": "off",
    "no-unused-vars":"warn",
    "no-useless-constructor":"off",
    "no-undef":"off",
    "no-extra-semi":"off",
    "no-extra-boolean-cast":"off",
    "no-await-in-loop":"off"
  }
}
